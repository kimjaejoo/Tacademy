package co.kr.taca.SpringFrameworkBasic;

public class day1_2 extends day1 {
	public static void main(String[] args) {
		System.out.println(day1.dayOne());
		System.out.println(day1.sumOne(12, 2));
	}
	// static한 method를 가지고 있는 class를 상속받아 부모클래스의 메소드를 사용가능하게 만들어준다.
	/**
	 * 두번째의 상위클래스의 메소드는 시험삼아 작성 = 동작해보면 어떻게 수행되는지 알게됨
	 */
}
